{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ThatInterpretingAgency;Trusted_Connection=true;TrustServerCertificate=true;MultipleActiveResultSets=true"
  },
  "ReverseProxy": {
    "Routes": {
      "api-agencies": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/agencies/{**catch-all}"
        }
      },
      "api-interpreters": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/interpreters/{**catch-all}"
        }
      },
      "api-clients": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/clients/{**catch-all}"
        }
      },
      "api-appointments": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/appointments/{**catch-all}"
        }
      },
      "api-billing": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/billing/{**catch-all}"
        }
      },
      "api-admin": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/admin/{**catch-all}"
        }
      },
      "api-notifications": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/notifications/{**catch-all}"
        }
      },
      "api-staff": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/staff/{**catch-all}"
        }
      },
      "api-interpreter-requests": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/interpreter-requests/{**catch-all}"
        }
      },
      "swagger-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/swagger/{**catch-all}"
        }
      },
      "api-catchall": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api-cluster": {
        "Destinations": {
          "api-destination": {
            "Address": "https://localhost:7058"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
